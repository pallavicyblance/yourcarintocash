<script type='text/javascript' src='https://www.bing.com/api/maps/mapcontrol?callback=getMap_address' async defer></script>

<script type='text/javascript'>
    function getMap_address() {
        Microsoft.Maps.loadModule('Microsoft.Maps.AutoSuggest', {
            callback: function () {
                var manager = new Microsoft.Maps.AutosuggestManager({
                    placeSuggestions: false
                });
                manager.attachAutosuggest('#locationNameinput', '.locationsearchBoxContainer', selectedSuggestion);
            },
            errorCallback: function(msg){
                //alert(msg);
            },
                   credentials: 'AsWsf1W37cp_tJ6fMTazsjLk9Ip0TxQyImLb2xzuWbVSYE9aRz_gEsYmakx-uibu'

        });
    }

    function selectedSuggestion(result) {
        // $("#states_drop").val(result.address.adminDistrict)
        // $('#states_drop').val(result.address.adminDistrict).trigger('change');
        console.log(result.address.adminDistrict);

        $('#states_drop [data-code="' + result.address.adminDistrict + '"]').prop("selected", true);
        $('#states_drop').trigger('change');


         setTimeout(function(){
            $('#locationNameinput').val(result.address.addressLine)
        }, 200);

        //$("#address1input").val(result.address.addressLine);
        $("#cityinput").val(result.address.locality);
        //$("#address2input").val(result.address.countryRegion);

    }
</script>



