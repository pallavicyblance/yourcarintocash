        $(function () {
            jQuery(".steps").hide();          
            jQuery("#steps_1").show();
            jQuery("#year_drop, #make_drop, #model_drop, #states_drop").select2(); 
            
            jQuery('#mileage').keyup(function() {
              jQuery(this).val(function(i, v) {
                return v.replace(/\D/g, "").replace(/\B(?=(\d{3})+(?!\d))/g, ",");
              });
            });
      
          if (jQuery(window).width() <= 767) {
            //console.log('mobile');
            jQuery(".responsive-sticky").on( "click", function() {
              jQuery( ".sidebar_title" ).slideToggle( "500", function() { });
            });
            jQuery(window).click(function() {
              jQuery( ".sidebar_title" ).slideUp();
            });
            jQuery('.content_sidebar').click(function(event){
              event.stopPropagation();
            });
          }
          else{
            //console.log('desktop');
          }
        });

        function showImage(){

          $('.car-tires-areas').show();
          $('#pdamagediv').hide();
          $('#no_repeat1').val('ok');
          $('#d_title').hide();
          $('#d_title1').show();
          $('#d_subtitle').hide();
          $('#d_subtitle1').show();
        }

        function onlyNumberKey(evt) {
          // Only ASCII character in that range allowed
          var ASCIICode = (evt.which) ? evt.which : evt.keyCode
          if (ASCIICode > 31 && (ASCIICode < 48 || ASCIICode > 57))
            return false;
          return true;
        }
    
