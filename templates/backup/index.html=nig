    <script>
            function displayImagePreviews(input) {
                var imagePreviews = document.getElementById('image-previews');
                imagePreviews.innerHTML = ''; // Clear any previous previews
    
                var length = input.files.length;
                if (length <= 4) {
                    var files = input.files;
                    for (var i = 0; i < files.length; i++) {
                        var file = files[i];
    
                        if (file.type.match('image.*')) {
                            var reader = new FileReader();
    
                            reader.onload = function(e) {
                                var img = document.createElement('img');
                                img.className = 'img-preview';
                                img.src = e.target.result;
                                imagePreviews.appendChild(img);
                            };
    
                            reader.readAsDataURL(file);
                        }
                    }
                    $("#d_file2_span").text("");
                    $("#d_file2_span").addClass("error");
                }else{
                    $("#d_file2_span").text("Select only four images.");
                    $("#d_file2_span").addClass("error");
                }
            }
    
            // Trigger the displayImagePreviews function when a file is selected
            document.getElementById('file-2').addEventListener('change', function(event) {
                displayImagePreviews(event.target);
            });
    
    
            function displayImagePreviews1(input) {
            var imagePreviews1 = document.getElementById('image-previews1');
            imagePreviews1.innerHTML = ''; // Clear any previous previews
    
            var length = input.files.length;
                if (length <= 1) {
                    var files = input.files;
                    for (var i = 0; i < files.length; i++) {
                        var file = files[i];
    
                        if (file.type.match('image.*')) {
                            var reader = new FileReader();
    
                            reader.onload = function(e) {
                                var img = document.createElement('img');
                                img.className = 'img-preview';
                                img.src = e.target.result;
                                imagePreviews1.appendChild(img);
                            };
    
                            reader.readAsDataURL(file);
                        }
                    }
                    $("#d_file3_span").text("");
                    $("#d_file3_span").addClass("error");
                }else{
                    $("#d_file3_span").text("Select only one image.");
                    $("#d_file3_span").addClass("error");
                }
        }
        document.getElementById('file-3').addEventListener('change', function(event) {
            displayImagePreviews1(event.target);
        });
